#include <iostream>
using namespace std;
class student
{
private:
    int a = 10, b = 20, c = 30;
    public:
    void display()
    {
        cout << "value of a=" << a << endl;
        cout << "value of a=" << b << endl;
        cout << "value of a=" << c << endl; 
    }
};
int main()
{
    int a, b, c;
    student s1, s2, s3;
    s1.display();
    return 0;
}